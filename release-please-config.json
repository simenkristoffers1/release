{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "bootstrap-sha": "66e9087f91cd336381ab12c3c85f021bf2794e22",
  "separate-pull-requests": true,
  "bump-minor-pre-major": true,
  "packages": {
    "assistant": {
      "component": "assistant",
      "release-type": "simple",
      "exclude-paths": [
        "src/DbUpMigrator"
      ],
      "initial-version": "0.0.1",
      "extra-files": [
        {
          "type": "xml",
          "path": "Directory.Build.props",
          "xpath": "/Project/PropertyGroup/Version"
        }
      ]
    },
    "assistant/src/DbUpMigrator": {
      "component": "migrations",
      "release-type": "simple",
      "initial-version": "0.0.1",
      "extra-files": [
        {
          "type": "xml",
          "path": "DbUpMigrator.csproj",
          "xpath": "/Project/PropertyGroup/Version"
        }
      ]
    },
    "infrastructure": {
      "component": "infrastructure",
      "release-type": "simple",
      "initial-version": "0.0.1"
    },
    "knowledge-search": {
      "component": "knowledge-search",
      "release-type": "python",
      "initial-version": "0.0.1"
    },
    "chat-ui": {
      "component": "chat-ui",
      "package-name": "chat-ui",
      "release-type": "node",
      "versioning": "prerelease",
      "prerelease": true,
      "prerelease-type": "beta",
      "initial-version": "0.0.1"
    }
  }
}